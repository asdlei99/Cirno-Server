{
  "name": "cirno",
  "version": "1.0.0",
  "description": "a wrapper for hbooker app interface ",
  "main": "app.js",
  "scripts": {
    "pkg": "pkg . --out-path=dist/"
  },
  "keywords": [
    "hbooker"
  ],
  "author": "zsakvo",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "http-server": "^0.12.3",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^9.1.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "open": "^7.1.0",
    "qs": "^6.9.4"
  },
  "bin": "./app.js",
  "pkg": {
    "scripts": [
      "./static/js/*.js"
    ],
    "assets": [
      "./static/index.html",
      "./static/favicon.ico",
      "./static/robots.txt",
      "./static/css/*",
      "./static/fonts/*",
      "./static/img/*"
    ],
    "targets": [
      "node8-macos-x64",
      "node8-win-x64",
      "node8-linux-x64"
    ]
  }
}
